      "type": {
        "type": "int",
        "logicalType": "date"
      },
      "doc": "Data do registro. Formato: ISO 8601. Cardinalidade: [1..1]"
    },
    {
      "name": "dataAlteracao",
      "type": {
        "type": "int",
        "logicalType": "date"
      },
      "doc": "Data da alteração do registro. Formato: ISO 8601. Cardinalidade: [1..1]"
    },
    {
      "name": "indicadorExclusao",
      "type": "int",
      "doc": "Indica se é um registro de exclusão. Valores: 1 - Sim, 2 - Não. Tamanho: 1. Cardinalidade: [1..1]"
    },
    {
      "name": "apoliceCodigo",
      "type": "string",
      "doc": "Identificador da Apólice / Bilhete. Tamanho máximo: 60. Cardinalidade: [1..1]"
    },
    {
      "name": "certificadoCodigo",
      "type": ["null", "string"],
      "default": null,
      "doc": "Identificador do Certificado. Tamanho máximo: 60. Cardinalidade: [0..1]"
    },
    {
      "name": "endossoCodigo",
      "type": ["null", "string"],
      "default": null,
      "doc": "Identificador do Endosso do documento. Tamanho máximo: 60. Cardinalidade: [0..1]"
