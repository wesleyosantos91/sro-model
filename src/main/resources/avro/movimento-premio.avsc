    {
      "name": "iof",
      "type": ["null", "double"],
      "default": null,
      "doc": "Valor de IOF da Cobertura em Reais. Formato: 16.2 decimais. Cardinalidade: [0..1]"
    },
    {
      "name": "custoAquisicao",
      "type": ["null", "double"],
      "default": null,
      "doc": "Custo de Aquisição do ramo ou da cobertura. Formato: 16.2 decimais. Cardinalidade: [0..1]"
    }
  ]
