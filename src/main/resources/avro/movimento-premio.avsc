    },
    {
      "name": "dataInicio",
      "type": {
        "type": "int",
        "logicalType": "date"
      },
      "doc": "Data de Início de Vigência de Prêmio da Cobertura. Formato: AAAA-MM-DD. Cardinalidade: [1..1]"
    },
    {
      "name": "dataTermino",
      "type": {
        "type": "int",
        "logicalType": "date"
      },
      "doc": "Data de Fim de Vigência de Prêmio da Cobertura. Formato: AAAA-MM-DD. Cardinalidade: [1..1]"
    },
    {
      "name": "limiteMaximoIndenizacao",
      "type": ["null", "double"],
      "default": null,
      "doc": "Limite Máximo de Indenização (LMI). Formato: 16.2 decimais. Cardinalidade: [0..1]"
    },
    {
      "name": "limiteMaximoIndenizacaoReal",
      "type": ["null", "double"],
      "default": null,
      "doc": "Limite Máximo de Indenização (LMI) em Reais. Formato: 16.2 decimais. Cardinalidade: [0..1]"
