    {
      "name": "status",
      "type": "int",
      "doc": "Status do processo de sinistro. Cardinalidade: [1..1]. Tag: status. Tipo: Int. Formato: 1 - Aberto 2 - Encerrado com Indenização 3 - Encerrado sem Indenização 4 - Reaberto 5 - Cancelado (Por erro operacional) 6 - Avaliação Inicial. Tamanho: 2"
    },
    {
      "name": "dataAlteracaoStatus",
      "type": {
        "type": "int",
        "logicalType": "date"
      },
      "doc": "Data da alteração do registro. Cardinalidade: [1..1]. Tag: data_alteracao_status. Tipo: Date. Formato: AAAA-MM-DD. Tamanho: 10"
    },
    {
      "name": "dataOcorrencia",
      "type": {
        "type": "int",
        "logicalType": "date"
